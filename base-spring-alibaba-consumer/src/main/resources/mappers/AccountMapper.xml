<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ibase.alibaba.consumer.dao.AccountDao">

	<update id="updateScore" parameterType="com.ibase.alibaba.consumer.entity.Account">
		update js_account set score =${score}
        where id=${id}
	</update>

</mapper>
